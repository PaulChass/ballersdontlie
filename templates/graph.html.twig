<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
<script>
window.onload = function () {
 
var chart = new CanvasJS.Chart("chartContainer", {
    exportEnabled: true,
	title: {
		text: "{{graph.title}}"
	},
	axisY: {
		title: "{{graph.stat}}"
	},
	data: [{
		type: "line",
			dataPoints: [
                {label:'{{graph.label.0}}',y:{{graph.value.0}}},
                {label:'{{graph.label.1}}',y:{{graph.value.1}}},
                {label:'{{graph.label.2}}',y:{{graph.value.2}}},
                {label:'{{graph.label.3}}',y:{{graph.value.3}}},
                {label:'{{graph.label.4}}',y:{{graph.value.4}}},
                {label:'{{graph.label.5}}',y:{{graph.value.5}}},
                {label:'{{graph.label.6}}',y:{{graph.value.6}}},
                {label:'{{graph.label.7}}',y:{{graph.value.7}}}
                ]
	}]
});
chart.render();
 
}
</script>
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %} {% endblock %}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
              crossorigin="anonymous">
        <link rel="stylesheet" href="/build/app.css">
        <style type="text/css">
        body{background-color:#d3d3d3;margin:0 80px}a:link,a:visited{color:grey}a:hover{color:#000}a:active{color:green}.card:hover{border-color:#000}.user_name{font-size:14px;font-weight:700}.comments-list .media{border-bottom:1px dotted #ccc}.center-div{margin:0 auto;width:80%}.left{text-align:left}td,th{font-size:small}.small-picture{max-height:30px}.form-group{opacity:.7;color:#fff}body{background-image:url(https://www.desktopbackground.org/download/o/2010/09/02/73539_basketball-wood-backgrounds_2560x1440_h.jpg)}#form_save{margin:auto;width:100%}.whitetitle{color:#f5f5f5;margin-top:50px;margin-bottom:10px}.result{font-size:1.3rem;text-align:right}
        </style>
        
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
                <link rel="icon" type="image/png" href="/basketball.png" />
        
    </head>
    <body>
     {% block body %}{% endblock %}
     
<div id="chartContainer" style="height: 100vh; width: 100%;"></div>
<script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>

    </body>

     {% block javascripts %}


<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
            {{ encore_entry_script_tags('app') }}

        {% endblock %}
</html>
